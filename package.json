{
  "name": "hackathon",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader",
    "build": "webpack --mode production --module-bind js=babel-loader",
    "server": "nodemon --exec babel-node -- ./bin/www",
    "start": "yarn run server"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "babel-preset-env": "1.7.0",
    "nodemon": "1.18.11",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0"
  }
}

{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "nodemon --exec babel-node ./bin/www",
    "start": "yarn run server"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "./dist/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.3",
    "nodemon": "1.18.11"
  }
}
